    "import torch\n",
    "a = torch.rand((3,3))\n",
    "print(a)\n",
    "print(a.size())\n",
    "print(a.stride())\n",
    "b = a.permute((1,0))\n",
    "print(b)\n",
    "print(b.size())\n",
    "print(b.stride())\n",
    "# now b is not contiguous"
